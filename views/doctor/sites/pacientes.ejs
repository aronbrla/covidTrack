<script>
    var form = document.getElementById("dni");
    var lista = document.getElementsByClassName("paciente");
    
    function filtrar(){
        
            let dni=form.value;
            console.log(dni);
            for(let x of lista){
                if(x.children[2].innerHTML.indexOf(dni)!==-1){
                    x.style.display="";
                    
                }else{
                    x.style.display="none";

                };
            }        
    }
    form.addEventListener("keyup",filtrar);
</script>
