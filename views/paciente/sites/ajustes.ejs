<%- include('../partials/parte_sup.ejs')%>
<div class="main-container flex-column bd-highlight mb-3 mx-auto">
    <div class="main-title justify-content-lg-start">
        <h1 class="text-light fs-2 fs-lg-5 text-center ms-lg-5">Editar Información Personal</h1>
    </div>
    <div class="information-container mt-sm-4">
        <form action="auth" method="POST" class="login-email">
            <ul class="list-group list-group-flush">
                <li class="list-group-item">
                    <span><i class='bx bxs-home bx-flip-horizontal'></i><span class="ms-2"></span>NOMBRES Y APELLIDOS</span></span>
                    <div class="input-group">
                        <input name="name" class="input-item" type="text" placeholder="Nombre" value="" autocomplete="none">
                    </div>
                </li>
                <li class="list-group-item">
                    <span><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-credit-card-fill" viewBox="0 0 16 16">
                        <path d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v1H0V4zm0 3v5a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7H0zm3 2h1a1 1 0 0 1 1 1v1a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1v-1a1 1 0 0 1 1-1z"/>
                      </svg><span class="ms-2">DNI</span></span> 
                    <div class="input-group">
                        <input name="dni" class="input-item" type="text" placeholder="DNI" value="" autocomplete="none">
                    </div>
                </li>
                <li class="list-group-item">
                    <span><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" width="16" height="16" fill="currentColor">
                        <path d="M4.75 0a.75.75 0 01.75.75V2h5V.75a.75.75 0 011.5 0V2h1.25c.966 0 1.75.784 1.75 1.75v10.5A1.75 1.75 0 0113.25 16H2.75A1.75 1.75 0 011 14.25V3.75C1 2.784 1.784 2 2.75 2H4V.75A.75.75 0 014.75 0zm0 3.5h8.5a.25.25 0 01.25.25V6h-11V3.75a.25.25 0 01.25-.25h2zm-2.25 4v6.75c0 .138.112.25.25.25h10.5a.25.25 0 00.25-.25V7.5h-11z"></path></svg><span class="ms-2">FECHA DE NACIMIENTO</span></span>
                    <div class="input-group">
                        <input name="date" class="input-item" type="date" placeholder="Fecha" value="" autocomplete="none">
                    </div>
                </li>
                <li class="list-group-item">
                    <span><svg aria-hidden="true" width="16" height="16" focusable="false" data-prefix="fas" data-icon="envelope" class="svg-inline--fa fa-envelope fa-w-16" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">
                        <path fill="currentColor" d="M502.3 190.8c3.9-3.1 9.7-.2 9.7 4.7V400c0 26.5-21.5 48-48 48H48c-26.5 0-48-21.5-48-48V195.6c0-5 5.7-7.8 9.7-4.7 22.4 17.4 52.1 39.5 154.1 113.6 21.1 15.4 56.7 47.8 92.2 47.6 35.7.3 72-32.8 92.3-47.6 102-74.1 131.6-96.3 154-113.7zM256 320c23.2.4 56.6-29.2 73.4-41.4 132.7-96.3 142.8-104.7 173.4-128.7 5.8-4.5 9.2-11.5 9.2-18.9v-19c0-26.5-21.5-48-48-48H48C21.5 64 0 85.5 0 112v19c0 7.4 3.4 14.3 9.2 18.9 30.6 23.9 40.7 32.4 173.4 128.7 16.8 12.2 50.2 41.8 73.4 41.4z"></path>
                        </svg><span class="ms-2">CORREO</span>
                    </span>
                    <div class="input-group">
                        <input name="mail" class="input-item" type="email" placeholder="Correo electronico" value="" autocomplete="none">
                    </div>
                </li>
                <li class="list-group-item">
                    <span><i class='bx bxs-lock-open-alt'></i><span class="ms-2">CONTRASEÑA</span></span> 
                    <div class="input-group">
                        <input name="pass" class="input-item" type="password" placeholder="Contraseña" value="" autocomplete="none">
                    </div>
                </li>
                <li class="list-group-item">
                    <span><i class='bx bxs-city' ></i><span class="ms-2">DISTRITO</span></span> 
                    <div class="input-group">
                        <input type="text" class="input-item" name="distrito" autocomplete="none">
                    </div>
                </li>
                <li class="list-group-item">
                    <span><i class='bx bxs-home bx-flip-horizontal'></i><span class="ms-2">DOMICILIO</span></span>
                    <div class="input-group">
                        <input name="address" class="input-item" type="text" placeholder="Dirección" value="" autocomplete="none" required>
                    </div>
                </li>
                <li class="list-group-item">
                    <span><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-telephone-fill" viewBox="0 0 16 16">
                        <path fill-rule="evenodd" d="M1.885.511a1.745 1.745 0 0 1 2.61.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.678.678 0 0 0 .178.643l2.457 2.457a.678.678 0 0 0 .644.178l2.189-.547a1.745 1.745 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.634 18.634 0 0 1-7.01-4.42 18.634 18.634 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877L1.885.511z"/>
                      </svg><span class="ms-2">TELEFONO/CELULAR</span></span> 
                    <div class="input-group">
                        <input name="phone" class="input-item" type="text" placeholder="Celular" value="" autocomplete="none">
                    </div>
                </li>
            </ul>
            <div class="edit d-flex justify-content-center justify-content-lg-end">
                <button id="submit" name="submit" class="btn btn-outline-info" style="height: 35px;">Guardar cambios</button>
            </div>
        </form>
    </div>
</div>
<%- include('../partials/parte_inf.ejs')%>